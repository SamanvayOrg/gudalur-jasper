<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="sc_ipd_pat_summary" pageWidth="595" pageHeight="842" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" isFloatColumnFooter="true" uuid="b0dc7381-9148-45f5-9537-daa5d58e2a61">
	<parameter name="from_date" class="java.util.Date"/>
	<parameter name="to_date" class="java.util.Date"/>
	<queryString>
		<![CDATA[select  rp.name,rpa.address2 as address1,sv.visit_startdate ,sv.visit_stopdate,
  case when sv.diagnoses IS NULL then ''
  else sv.diagnoses END as diagnosis,sum(so.amount_total)  as cost,
  case when sum(so.amount_total)<2500 then sum(so.amount_total)
  else 2500
  end as claim_amount
from syncjob_visit sv
  inner join syncjob_sickle_cell_patient sscp on sscp.erp_id=sv.erp_patient_id AND
        sv.visit_type_id=1
        and cast(sv.visit_stopdate as date) BETWEEN $P{from_date} AND $P{to_date}
  INNER JOIN res_partner rp on rp.id=sscp.erp_id
  inner join claim_type ct on ct.erp_patient_id=rp.id and ct.claim_type='1'
  inner JOIN res_partner_address rpa on rp.id = rpa.partner_id
  inner join sale_order so ON so.partner_id=rp.id and so.state!='draft'
    and cast(so.create_date as date) BETWEEN $P{from_date} AND $P{to_date}
GROUP BY sv.visit_startdate,sv.visit_stopdate,sv.diagnoses,sv.erp_patient_id,rp.name,rpa.address2]]>
	</queryString>
	<field name="name" class="java.lang.String"/>
	<field name="address1" class="java.lang.String"/>
	<field name="visit_startdate" class="java.sql.Timestamp"/>
	<field name="visit_stopdate" class="java.sql.Timestamp"/>
	<field name="diagnosis" class="java.lang.String"/>
	<field name="cost" class="java.math.BigDecimal"/>
	<field name="claim_amount" class="java.math.BigDecimal"/>
	<variable name="amount_sum" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{claim_amount}]]></variableExpression>
	</variable>
